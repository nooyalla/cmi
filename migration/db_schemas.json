{
    "tables": [
        {
            "tableName":  "events",
            "columns" : [
                "id text PRIMARY KEY",
                "title text NOT NULL",
                "description text",
                "location text",
                "image_url text",
                "start_date timestamp without time zone NOT NULL",
                "end_date timestamp without time zone",
                "last_confirmation_date timestamp without time zone NOT NULL",
                "min_participants integer default 0",
                "max_participants integer default 0",
                "created_at timestamp without time zone NOT NULL",
                "updated_at timestamp without time zone NOT NULL",
                "deleted_at timestamp without time zone"
            ]
        },
        {
            "tableName":  "users",
            "columns" : [
                "id text PRIMARY KEY",
                "first_name text",
                "family_name text",
                "email text",
                "image_url text",
                "token text",
                "token_expiration timestamp without time zone NOT NULL",
                "created_at timestamp without time zone NOT NULL",
                "updated_at timestamp without time zone NOT NULL",
                "deleted_at timestamp without time zone"
            ]
        },
        {
            "tableName":  "event_users",
            "columns" : [
                "id text PRIMARY KEY NOT NULL",
                "user_id text NOT NULL",
                "event_id text NOT NULL",
                "confirmation_date timestamp without time zone NOT NULL",
                "created_at timestamp without time zone NOT NULL",
                "updated_at timestamp without time zone NOT NULL",
                "deleted_at timestamp without time zone"
            ]
        }
    ]
}

